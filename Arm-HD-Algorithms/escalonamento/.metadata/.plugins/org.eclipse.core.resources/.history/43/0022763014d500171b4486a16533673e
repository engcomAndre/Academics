	}
	
	public static int getDiscArmInitialCilinder() {
		return discArmInitialCilinder;
	}
	
	public static LinkedList<Integer> getProcess() {
		return Process;
	}
	
	//Sets
	public void setQntdCiliders(int qntdCiliders) {
		this.qntdCilinder = qntdCiliders;
	}
	
	public void setDiscArmInitialCilinder(int discArmInitialCilinder) {
		Configure.discArmInitialCilinder = discArmInitialCilinder;
	}
	
	public void setSeekTime(float seekTime) {
		Configure.seekTime = seekTime;
	}
	
	public void setProcess() {
		//Process = DataIN.getData();
	}		
}
