aprenda main1
   comlimite
   atrlocal "tat  9
   atrlocal "mun 20
   atrlocal "muni 0
   mud_rp
  mudefonte 50
 
                       enquanto  [não :tat = 0] [
                                                atat  0 mira
                                                 contagemm :muni  
                                                 contagem :tat
                                                 atat :tat    
                                                 mostretat fuga_tat comp_pos espere 6
                                                 se (e :posx = :posxm  :posy = :posym étecla? ) 
                                                       [se  (e leiacar = 32  :muni >0) 
                                                                                    [mate_tat :tat
                                                                                     atribua "muni :muni+1
                                                                                     atribua "tat :tat -1
                                                                                      ]
                                                       ]                                                                                                
                                                                   #toquemp3" (som de arma em falso)
                                                       se :tat = 1 [ contagem 0 
                                                                          mensagem 
                                                                          [Você Agora é um Serial Killer de Tartarugas Nível 1] 
                                                                           espere 30
                                                                          Leia [DIGITE QUALQUER TECLA PARA CONTINUAR DESEJA IR AO PRÓXIMO NÍVEL OU "X" PARA SAIR] "aval
                                                                          SE (:AVAL = car 120 ou :aval = car 88) [ADEUS]


                                                                          ]
                                                          
                                                     se étecla? 
                                                     [se leiacar  =32 
                                                            [atribua "muni :muni+1
                                                             toquemp3 "Shotgun-ShootReload.mp3]                                                      
                                                      ]                          
                                                      ]                                                                                                                               
fim

