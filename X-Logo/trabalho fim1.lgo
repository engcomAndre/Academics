aprenda comp_pos
   atrlocal "prec 100
    pf 25 pd 8
    atr "posx     inteiro(elem 1 pos)/:prec 
    atr "posy     inteiro(elem 2 pos)/:prec
    atr "posxm  inteiro(elem 1 posmouse)/:prec
    atr "posym  inteiro(elem 2 posmouse)/:prec
           
          
                                   
